{
	"manifest_version": 2,
	"name": "Datetime",
	"version": "1.0",
	"description": "This extension displays the date and time as an extension popup",
	"browser_action": {
		"default_title": "Datetime",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"icons" : {
		"16" : "icon16.png",
		"48" : "icon48.png",
		"128" : "icon128.png"
	}

}